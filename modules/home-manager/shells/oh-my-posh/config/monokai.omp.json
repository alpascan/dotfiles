{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "foreground": "#F8F8F2",
                    "style": "plain",
                    "template": "{{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
                    "type": "os"
                  },
                {
                    "foreground": "#FFD866",
                    "properties": {
                        "time_format": "15:04"
                    },
                    "style": "plain",
                    "template": "{{ .CurrentDate | date .Format }}",
                    "type": "time"
                },
                {
                    "type": "shell",
                    "style": "plain",
                    "foreground": "#F92672",
                    "properties": {
                        "mapped_shell_names": {
                            "pwsh": "PowerShell",
                            "powershell": "PowerShell",
                            "cmd": "Cmd",
                            "bash": "Bash",
                            "nu": "Nushell",
                            "fish": "Fish"
                        }
                    },
                    "template": " \ue795 {{ .Name }} "
                },
                {
                    "type": "kubectl",
                    "style": "plain",
                    "foreground": "#AE81FF",
                    "template": "\uf308 {{.Context}}{{if .Namespace}} -> {{.Namespace}}{{end}} ",
                    "properties": {
                        "display_error": true
                    }
                },
                {
                    "foreground": "#C39AC9",
                    "foreground_templates": [
                        "{{ if or (.Working.Changed) (.Staging.Changed) }}#FC9867{{ end }}",
                        "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#FC618D{{ end }}",
                        "{{ if gt .Ahead 0 }}#C39AC9{{ end }}",
                        "{{ if gt .Behind 0 }}#C39AC9{{ end }}"
                    ],
                    "properties": {
                        "branch_max_length": 25,
                        "fetch_stash_count": true,
                        "fetch_status": true,
                        "fetch_upstream_icon": true
                    },
                    "style": "plain",
                    "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
                    "type": "git"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "right",
            "segments": [
                {
                    "type": "status",
                    "style": "plain",
                    "foreground": "#F8F8F2",
                    "foreground_templates": [
                        "{{ if gt .Code 0 }}#FC618D{{ end }}"
                    ],
                    "template": "\uf00d x{{ reason .Code }}"
                },
                {
                    "foreground": "#F8F8F2",
                    "foreground_templates": [
                        "{{ if gt .Code 0 }}#FC618D{{ end }}"
                    ],
                    "properties": {
                        "style": "roundrock",
                        "always_enabled": true
                    },
                    "style": "diamond",
                    "template": "\uf0e7 {{ .FormattedMs }} ",
                    "type": "executiontime"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "foreground": "#FF9B50",
                    "properties": {
                        "style": "full"
                    },
                    "style": "plain",
                    "template": "\uf07c {{ .Path }}",
                    "type": "path"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "foreground": "#F8F8F2",
                    "style": "plain",
                    "template": "\uf120",
                    "type": "root"
                },
                {
                    "type": "python",
                    "style": "plain",
                    "foreground": "#66D9EF",
                    "template": "\uE235 {{ .Full }}"
                },
                {
                    "type": "node",
                    "style": "plain",
                    "foreground": "#A6E22E",
                    "template": "\ue718 {{ .Full }}"
                },
                {
                    "type": "java",
                    "style": "plain",
                    "foreground": "#F92672",
                    "template": "\ue256 {{ if .Full }}{{ .Full }}{{ else }}Java{{ end }}",
                    "properties": {
                        "fetch_version": true,
                        "display_version": true,
                        "display_mode": "files",
                        "home_enabled": true
                    }
                },
                {
                    "type": "go",
                    "style": "plain",
                    "foreground": "#A6E22E",
                    "template": "\uFCD1 {{ if .Full }}{{ .Full }}{{ else }}Go{{ end }}"
                },
                {
                    "foreground": "#F8F8F2",
                    "style": "plain",
                    "template": " \u276f",
                    "type": "text"
                }
            ],
            "type": "prompt"
        }
    ],
    "final_space": true,
    "version": 3,
    "transient_prompt": {  
    "background": "transparent",
    "foreground": "#F8F8F2",
    "template": " \u276f "
    }
}